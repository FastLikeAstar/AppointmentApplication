<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane alignment="CENTER" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ContactAppointmentReportController">
<VBox prefHeight="720.0" >
   <children>
      <Separator prefHeight="42.0" visible="false" />
      <Label text="Contact Appointment Breakdown Report">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Separator prefHeight="52.0" visible="false" />
      <HBox prefHeight="52.0" spacing="10.0" />
      <TableView fx:id="TableContactInfo" prefHeight="396.0">
         <columns>
            <TableColumn fx:id="ColumnMonth" prefWidth="93.0" text="Month" />
            <TableColumn fx:id="ColumnContactName" prefWidth="161.0" text="Contact" />
            <TableColumn fx:id="ColumnAppointmentType" prefWidth="215.0" text="Appointment Type" />
            <TableColumn fx:id="ColumnNumOfAppointments" prefWidth="272.0" text="Total Number of Appointments" />
            <TableColumn fx:id="ColumnTimeForAppointments" prefWidth="384.0" text="Time Scheduled for Appointments" />
         </columns>
      </TableView>
      <Separator prefHeight="35.0" visible="false" />
      <Button fx:id="ButtonBack" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#BackToReportsMenu" prefHeight="40.0" prefWidth="149.0" text="Back to Reports Menu">
         <VBox.margin>
            <Insets left="900.0" />
         </VBox.margin>
      </Button>
      <Separator prefHeight="59.0"  visible="false" />
   </children>
</VBox>
</StackPane>
